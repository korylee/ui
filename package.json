{
  "name": "ui",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm run clean && cross-env NODE_ENV=development vite",
    "build": "vue-tsc --noEmit && vite build",
    "clean": "rimraf site lib es node_modules/naive-ui themes/**/es themes/**/lib",
    "clean:lib": "rimraf node_modules",
    "lint:code": "eslint \"{src,build,scripts,demo}/**/*.{ts,tsx,js,vue,md}\"",
    "lint:fix": "eslint --fix \"{src,build,scripts,demo}/**/*.{ts,tsx,js,vue,md}\"",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/generator": "^7.18.2",
    "@babel/parser": "^7.18.4",
    "@babel/preset-env": "^7.18.2",
    "@babel/traverse": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/server-renderer": "^3.2.37",
    "@vue/test-utils": "2.0.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.11",
    "esbuild": "^0.14.43",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "fs-extra": "^10.1.0",
    "jsdom": "^19.0.0",
    "lodash.camelcase": "^4.3.0",
    "marked": "^4.0.16",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "typescript": "^4.7.3",
    "vfonts": "^0.1.0",
    "vite": "3.0.0-beta.1",
    "vitest": "^0.14.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15",
    "vue-tsc": "^0.34.17"
  },
  "dependencies": {
    "@css-render/plugin-bem": "^0.15.10",
    "@css-render/vue3-ssr": "^0.15.10",
    "@types/lodash-es": "^4.17.6",
    "async-validator": "^4.1.1",
    "css-render": "^0.15.10",
    "csstype": "^3.1.0",
    "date-fns": "^2.28.0",
    "highlight.js": "^11.5.1",
    "lodash-es": "^4.17.21",
    "seemly": "^0.3.4",
    "vooks": "^0.2.12"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "keywords": [
    "component library",
    "ui",
    "vue"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "postcss",
        "rollup",
        "webpack"
      ],
      "allowedVersions": {
        "eslint": "^8.6.0",
        "eslint-plugin-promise": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "eslint-plugin-vue": "^9.1.0"
      }
    }
  }
}
